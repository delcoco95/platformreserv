{
  "name": "bookauto",
  "version": "1.0.0",
  "description": "BookAuto - Plateforme de r√©servation automobile",
  "scripts": {
    "dev:db": "docker compose up -d mongodb",
    "dev:backend": "cd backend && nodemon server.js",
    "dev:frontend": "cd frontend && vite --port 3000",
    "dev": "npm run dev:db && npm run dev:backend",
    "start": "npm run start:db && npm run start:backend",
    "start:db": "docker compose up -d mongodb",
    "start:backend": "cd backend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "clean": "docker compose down -v",
    "setup": "npm run install:all && npm run dev:db"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "keywords": ["bookauto", "automobile", "reservation", "booking"],
  "author": "BookAuto Team",
  "license": "MIT"
}